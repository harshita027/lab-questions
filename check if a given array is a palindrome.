#include <stdio.h>

int main() {
    int a[50],n,i,j;
    printf("\nenter no. of elements:");
    scanf("%d",&n);
    
    printf("\nenter number:");
    
    for(i=0;i<n;i++)
    {
    scanf("%d",&a[i]);
    }
    
    for(i=0,j=n-1;i<j;i++,j--)
    {
        if(a[i]=a[j])
        {
            printf("\narray is palindrome");
        }
        else
        {
            printf("\narray is not palindrome");
        }
    }
    
    return 0;
}
